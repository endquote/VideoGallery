{
  "name": "video-gallery-client",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Josh Santangelo <josh@endquote.com>",
  "description": "Client for the Video Gallery application.",
  "license": "MIT",
  "scripts": {
    "start": "node ./node_modules/watchify/bin/cmd.js main.js -o 'node ./node_modules/exorcist/bin/exorcist.js bundle.js.map > bundle.js' -v -d",
    "build": "node ./node_modules/browserify/bin/cmd.js --entry main.js --debug | node ./node_modules/exorcist/bin/exorcist.js bundle.js.map > bundle.js",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "exorcist": "^0.4.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "superagent": "^3.4.1",
    "vue": "^2.1.10"
  }
}
